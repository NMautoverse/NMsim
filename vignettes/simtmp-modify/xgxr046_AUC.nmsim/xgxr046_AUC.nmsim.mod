$PROBLEM    043 as 1CMT ADVAN13 with AMT in micrograms
$INPUT ROW ID TIME EVID CMT AMT II ADDL DV MDV

$DATA ./NMsimData_xgxr046_AUC.nmsim.csv
IGN=@

$SUBROUTINE ADVAN13 TOL 9
$MODEL      COMP=(DEPOT,DEFDOSE) COMP=(CENTRAL,DEFOBS)
;;       COMP=(PERIPH)
COMP=(AUC)

$PK
LTVKA=THETA(1)
LTVV2=THETA(2)
LTVCL=THETA(3)
;; LTVV3=THETA(4)
;; LTVQ=THETA(5)

MU_1=LTVKA
KA=EXP(MU_1+ETA(1))
MU_2=LTVV2
V2=EXP(MU_2+ETA(2))
MU_3=LTVCL
CL=EXP(MU_3+ETA(3))
;; MU_4=LTVV3
;; V3=EXP(MU_4+ETA(4))
;; MU_5 = LTVQ
;; Q =EXP(MU_5+ETA(5))
;; S2=V2/1000

$DES
DADT(1) = -KA*A(1)
DADT(2) = KA*A(1) - CL*A(2)/V2  ;;+ Q*(A(3)/V3-A(2)/V2)
;;DADT(3) = -Q*(A(3)/V3-A(2)/V2)


DADT(3)=A(2)/V2

$ERROR
IPRED=A(2)/V2
IRES=DV-IPRED

;; IF (IPRED.GT.1) THEN
;;   W = SQRT(IPRED**2*SIGMA(1,1) + SIGMA(2,2))
;; ELSE
;;   W=1
;; ENDIF

;; IWRES=IRES/W
Y=IPRED+IPRED*ERR(1)+ERR(2)

;-----------------------INITIAL ESTIMATES---------------------------------
AUCCUM=A(3)
IF(NEWIND.NE.2) OLDAUCCUM=0
AUCNMSIM = AUCCUM-OLDAUCCUM
OLDAUCCUM = AUCCUM

$THETA  1.11975 ; LTVKA
$THETA  11.4649 ; LTVV2
$THETA  9.8312 ; LTVCL
;; $THETA  (5)          FIX   ; LTVV3
;; $THETA  (-1000) FIX             ; LTVQ
$OMEGA  0.0153241
$OMEGA  0.137697
$OMEGA  0.165658
;; $OMEGA 0 FIX
;; $OMEGA 0 FIX
$SIGMA  0.091529
$SIGMA  0.00278656


$SIMULATION ONLYSIM (12345) 

$TABLE PRED IPRED AUCNMSIM NOPRINT NOAPPEND FILE=xgxr046_AUC.nmsim.tab

